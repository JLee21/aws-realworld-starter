{
  "name": "aws-realworld-starter",
  "version": "1.0.0",
  "description": "AWS Serverless typescript starter kit using webpack",
  "scripts": {
    "test": "DYNAMO_TABLE=sweeeet nyc mocha -r reflect-metadata -r tests/prepare.ts tests/**/*.unit.spec.ts",
    "test:e2e": "nyc mocha tests/**/e2e.spec.ts --timeout 10000",
    "test:e2e:postman": "./node_modules/newman/bin/newman.js run tests/postman/api-tests.postman.json -e ./tests/postman/env-api-tests.postman.json",
    "test:e2e:deploy": "./node_modules/serverless/bin/serverless deploy && gulp && npm run test:e2e",
    "test:e2e:remove": "./node_modules/serverless/bin/serverless remove"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Russ Watson",
  "repository": {
    "url": "https://github.com/bakerstreet-industries/aws-realworld-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "inversify": "^4.5.1",
    "jsonwebtoken": "^8.2.1",
    "reflect-metadata": "^0.1.12",
    "ts-log-class": "^1.0.15",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.20",
    "@types/chai": "^4.1.3",
    "@types/crypto-js": "^3.1.39",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.13",
    "@types/reflect-metadata": "0.0.5",
    "@types/supertest": "^2.0.3",
    "@types/uuid": "^3.4.3",
    "aws-sdk": "^2.244.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-string-replace": "^0.4.0",
    "mocha": "^4.1.0",
    "newman": "^3.9.3",
    "nyc": "^11.7.3",
    "serverless": "^1.27.2",
    "serverless-plugin-typescript": "^1.1.5",
    "supertest": "^3.0.0",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3"
  }
}
